<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Projects</title>
</head>
<body>
  <h1>All Users</h1>
  <ul>
    <% users.forEach(user => { %>
        <h3><%= user.username %></h3>
        <ul>
        <% user.tasks.forEach(task => { %>
            <li><%= task.title%></li>
          <% }); %>
        </ul>
    <% }); %>
  </ul>
</body>
</html>
